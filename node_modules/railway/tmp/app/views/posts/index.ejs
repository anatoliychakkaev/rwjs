<h1>Index of post</h1>

<% posts.forEach(function (post) { %>
  post #<%= post.id %>
  <%- link_to('Edit', path_to.edit_post(post)) %>
  <%- link_to('Delete', path_to.post(post), {method: 'delete', remote: true, jsonp: '(function (u) {location.href = u;})'}) %><br/>
<% }); %>

<%- link_to('New post', path_to.new_post) %>
