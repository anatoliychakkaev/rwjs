.page-header
  h1 Index of models

- models.forEach(function (model) {
  .row
    .span-two-thirds= 'model #' + model.id
    .span-one-third
      != link_to('Edit', path_to.edit_model(model), {class: 'btn primary'}) + ' '
      != link_to('Delete', path_to.model(model), {class: 'btn danger', method: 'delete', remote: true, jsonp: '(function (u) {location.href = u;})'})
- });

hr

.well
  != link_to('New model', path_to.new_model, {class: 'btn'})
